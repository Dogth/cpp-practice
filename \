#include <utility>

bool solveBoolean11(int a, int b){
  return a % 2 == b % 2;
}

bool solveBoolean20(int number){
  return number / 100 == number % 10 ||
    (number / 10) % 10 == number % 10 ||
    number / 10 == (number / 10) % 10;
}

class rectangle {
  public:
  std::pair<int,int> coord1;
  std::pair<int,int> coord2;
  
  rectangle(&auto[x1, y1], &auto[x2, y2])
  {
    coord1{x1, y1};
    coord2{x2, y2};
  }
};

bool solveBoolean29(rectangle recta, &auto[x,y]){
  return {x,y} <=rect.coord1 && {x,y} >= rect.coord2; 
}
